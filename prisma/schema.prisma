generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Theme {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  label       String @unique
  iconName    String
  description String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}
